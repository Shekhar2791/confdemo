									<div class="item">
									<img src="images/presentation/5.png">
									</div>
								</div>
								
							</div>
							
							<!-- SLIDER -->


						</div>
						<!-- //PRESENTATION SLIDER -->	
						<!-- SLIDE CONTROLS -->
						<div class="sliderControls">
							<div class="prevSlide" href="#presentation-carousel" data-slide="prev">
								<span class="glyphicon glyphicon-chevron-left"></span>
								<span class="textSpan">Prev</span></div> 
							
							<div class="nextSlide" href="#presentation-carousel" data-slide="next">
								<span class="textSpan">Next</span>
								<span class="glyphicon glyphicon-chevron-right"></span>
							</div> 
							
							<ul class="slideCount">
							    <li data-target="#presentation-carousel" data-slide-to="0">1</li>
							    <li data-target="#presentation-carousel" data-slide-to="1">2</li>
							    <li data-target="#presentation-carousel" data-slide-to="2">3</li>
							    <li data-target="#presentation-carousel" data-slide-to="3">4</li>
							    <li data-target="#presentation-carousel" data-slide-to="4">5</li>
							</ul>
							
							
						</div>
